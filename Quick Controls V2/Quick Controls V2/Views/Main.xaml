						</Grid>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
		<Style x:Key="VolumeSliderStyle" TargetType="Slider">
			<Setter Property="BorderThickness" Value="0"/>
			<Setter Property="BorderBrush" Value="Transparent"/>
			<Setter Property="Maximum" Value="10"/>
			<Setter Property="Minimum" Value="0"/>
			<Setter Property="Value" Value="0"/>
			<Setter Property="Background" Value="{StaticResource PhoneChromeBrush}"/>
			<Setter Property="Foreground" Value="{StaticResource PhoneAccentBrush}"/>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="Slider">
						<Grid Background="Transparent">
							<VisualStateManager.VisualStateGroups>
								<VisualStateGroup x:Name="CommonStates">
									<VisualState x:Name="Normal"/>
									<VisualState x:Name="MouseOver"/>
									<VisualState x:Name="Disabled">
										<Storyboard>
											<DoubleAnimation Duration="0" To="0.1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="HorizontalTrack"/>
											<DoubleAnimation Duration="0" To="0.1" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="VerticalTrack"/>
											<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Fill" Storyboard.TargetName="HorizontalFill">
												<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource PhoneDisabledBrush}"/>
											</ObjectAnimationUsingKeyFrames>
											<ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Fill" Storyboard.TargetName="VerticalFill">
												<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource PhoneDisabledBrush}"/>
											</ObjectAnimationUsingKeyFrames>
										</Storyboard>
									</VisualState>
								</VisualStateGroup>
							</VisualStateManager.VisualStateGroups>
							<Grid x:Name="HorizontalTemplate" Margin="{StaticResource PhoneHorizontalMargin}">
								<Rectangle x:Name="HorizontalTrack" Fill="{TemplateBinding Background}" Height="12" IsHitTestVisible="False" Margin="0,22,0,50"/>
								<Rectangle x:Name="HorizontalFill" Fill="{TemplateBinding Foreground}" Height="12" IsHitTestVisible="False" Margin="0,22,0,50">
									<Rectangle.Clip>
										<RectangleGeometry Rect="0, 0, 6, 12"/>
									</Rectangle.Clip>
								</Rectangle>
								<Rectangle x:Name="HorizontalCenterElement" HorizontalAlignment="Left" Height="24" Margin="0,16,0,44" Width="12">
									<Rectangle.Fill>
										<SolidColorBrush Color="White"/>
									</Rectangle.Fill>
									<Rectangle.RenderTransform>
										<TranslateTransform/>
									</Rectangle.RenderTransform>
								</Rectangle>
							</Grid>
							<Grid x:Name="VerticalTemplate" Margin="{StaticResource PhoneVerticalMargin}">
								<Rectangle x:Name="VerticalTrack" Fill="{TemplateBinding Background}" IsHitTestVisible="False" Margin="18,0,18,0" Width="12"/>
								<Rectangle x:Name="VerticalFill" Fill="{TemplateBinding Foreground}" IsHitTestVisible="False" Margin="18,0,18,0" Width="12">
									<Rectangle.Clip>
										<RectangleGeometry Rect="0, 0, 12, 6"/>
									</Rectangle.Clip>
								</Rectangle>
								<Rectangle x:Name="VerticalCenterElement" Fill="{StaticResource PhoneForegroundBrush}" Height="12" Margin="12,0,12,0" VerticalAlignment="Top" Width="24">
									<Rectangle.RenderTransform>
										<TranslateTransform/>
									</Rectangle.RenderTransform>
								</Rectangle>
							</Grid>
						</Grid>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
	</phone:PhoneApplicationPage.Resources>

    <!--LayoutRoot is the root grid where all page content is placed-->
    <Grid x:Name="LayoutRoot" Background="#FF1C2228">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
    	<StackPanel x:Name="Main1">
    		<TextBlock x:Name="Copyright" HorizontalAlignment="Left" TextWrapping="Wrap" Text="#inception"
    			VerticalAlignment="Top" FontFamily="Segoe WP Light" Margin="10,10,0,0" Tap="Copyright_Tap" Foreground="White" />
    		<Grid x:Name="Mediaplayer" Height="250" Background="#FF1F252B" Margin="20">
    			<ScrollViewer x:Name="CoverScrollViewer" Margin="2" UseLayoutRounding="True" IsEnabled="False">
    				<Rectangle x:Name="Cover">
    					<Rectangle.Projection>
    						<PlaneProjection />
    					</Rectangle.Projection>
    				</Rectangle>
    			</ScrollViewer>
    			<Rectangle x:Name="CoverFront" Stroke="Black" Margin="2" Opacity="0.2" Fill="Black" />
    			<Grid x:Name="StopMusicButton" VerticalAlignment="Bottom" HorizontalAlignment="Right"
    				Margin="20,0,0,20" Width="80" Height="80">
    				<Button x:Name="Stop" Content="Button" HorizontalAlignment="Center" Height="50" Margin="5"
    					Style="{StaticResource ButtonStyle17}" VerticalAlignment="Center" Width="50"
    					Click="Stop_Click" Hold="Stop_Hold" />
    			</Grid>

    			<StackPanel x:Name="MediaControls" Orientation="Horizontal" HorizontalAlignment="Left"
    				VerticalAlignment="Bottom" Margin="20,0,0,20">
    				<Button x:Name="Previous" Content="Button" Height="50" Style="{StaticResource ButtonStyle13}"
    					Width="50" Margin="5" Click="Previous_Click" Hold="Previous_Hold" />
    				<Button x:Name="Play" Content="Button" Height="70" Style="{StaticResource ButtonStyle14}"
    					Width="70" Margin="5" Click="Play_Click" Hold="Play_Hold" />
    				<Button x:Name="Pause" Content="Button" Height="70" Style="{StaticResource ButtonStyle15}"
    					Width="70" Visibility="Collapsed" Margin="5" Click="Pause_Click" Hold="Pause_Hold" />
    				<Button x:Name="Next" Content="Button" Height="50" Style="{StaticResource ButtonStyle16}"
    					Width="50" Margin="5" Click="Next_Click" Hold="Next_Hold" />
    			</StackPanel>
    			<StackPanel x:Name="InfoSong" Margin="20,20,0,0">
    				<TextBlock x:Name="NameSong" TextWrapping="Wrap" VerticalAlignment="Top"
    					FontFamily="Segoe WP Light" HorizontalAlignment="Left" Foreground="White"/>
    				<TextBlock x:Name="ArtistsSong" HorizontalAlignment="Left" TextWrapping="Wrap"
    					VerticalAlignment="Top" FontSize="16" FontFamily="Segoe WP Light" Foreground="White"/>
    			</StackPanel>

    		</Grid>
    		<StackPanel x:Name="AdjustVolume" Orientation="Horizontal" HorizontalAlignment="Center"
    			VerticalAlignment="Center">

    			<Button x:Name="Mute" Content="Button" HorizontalAlignment="Left" Height="50"
    				Style="{StaticResource ButtonStyle11}" VerticalAlignment="Top" Width="50" Click="Mute_Click"
    				Hold="Mute_Hold" />

    			<Slider x:Name="VolumeSlider" Background="#FF7B7B7B" Maximum="30" SmallChange="1" Value="15"
    				Width="300" ValueChanged="VolumeSlider_ValueChanged"
    				ManipulationCompleted="VolumeSlider_ManipulationCompleted" Foreground="White" Style="{StaticResource VolumeSliderStyle}" />
    			<StackPanel Height="50" Orientation="Horizontal" VerticalAlignment="Top">
    				<Button x:Name="Volume" Content="Button" HorizontalAlignment="Left" Height="50"
    					Style="{StaticResource ButtonStyle12}" VerticalAlignment="Top" Width="50"
    					Click="Volume_Click" Hold="Volume_Hold" />
    				<TextBlock x:Name="VolumeValue" TextWrapping="Wrap" Text="😊" HorizontalAlignment="Center"
    					VerticalAlignment="Center" FontFamily="Segoe WP Light" FontSize="26.667" Width="30" Foreground="White" />

    			</StackPanel>

    		</StackPanel>
    		<ScrollViewer VerticalAlignment="Top" Height="350">
    			<StackPanel x:Name="QuickAccess" Orientation="Vertical">
    				<Grid x:Name="Menu1" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="2,2,2,0">
    					<StackPanel x:Name="Menu1Sub" Orientation="Horizontal" HorizontalAlignment="Center"
    						VerticalAlignment="Center">
    						<Button x:Name="Wifi" Content="Button" HorizontalAlignment="Center" Height="100"
    							Style="{StaticResource Style}" VerticalAlignment="Center" Width="100"
    							Click="Wifi_Click" Hold="Wifi_Hold" Margin="5" />
    						<Button x:Name="Cellular" Content="Button" HorizontalAlignment="Center" Height="100"
    							Style="{StaticResource ButtonStyle1}" VerticalAlignment="Center" Width="100"
    							Click="Cellular_Click" Hold="Cellular_Hold" Margin="5" />
    						<Button x:Name="Bluetooth" Content="Button" HorizontalAlignment="Center" Height="100"
    							Style="{StaticResource ButtonStyle2}" VerticalAlignment="Center" Width="100"
    							Click="Bluetooth_Click" Hold="Bluetooth_Hold" Margin="5" />
    						<Button x:Name="Location" Content="Button" HorizontalAlignment="Center" Height="100"
    							Style="{StaticResource ButtonStyle3}" VerticalAlignment="Center" Width="100"
    							Click="Location_Click" Hold="Location_Hold" Margin="5" />
    					</StackPanel>
    					<StackPanel x:Name="State1" Orientation="Horizontal" VerticalAlignment="Bottom"
    						HorizontalAlignment="Center">
    						<Rectangle x:Name="WifiState" Fill="{StaticResource PhoneAccentBrush}"
    							HorizontalAlignment="Center" Height="10" VerticalAlignment="Bottom" Width="100"
    							Margin="5" Opacity="0" />
    						<Rectangle x:Name="CellularState" Fill="{StaticResource PhoneAccentBrush}"
    							HorizontalAlignment="Center" Height="10" VerticalAlignment="Bottom" Width="100"
    							Margin="5" Opacity="0" />
    						<Rectangle x:Name="BluetoothState" Fill="{StaticResource PhoneAccentBrush}"
    							HorizontalAlignment="Center" Height="10" VerticalAlignment="Bottom" Width="100"
    							Margin="5" Opacity="0" />
    						<Rectangle x:Name="LocationState" Fill="{StaticResource PhoneAccentBrush}"
    							HorizontalAlignment="Center" Height="10" VerticalAlignment="Bottom" Width="100"
    							Margin="5" Opacity="0" />
    					</StackPanel>

    				</Grid>
    				<StackPanel x:Name="Menu2" Orientation="Horizontal" HorizontalAlignment="Center"
    					VerticalAlignment="Center" Margin="2,0">
    					<Grid x:Name="State2" Height="100" Width="100" Margin="5">
    						<Button x:Name="Airplane" Content="Button" HorizontalAlignment="Center" Height="100"
    							Style="{StaticResource ButtonStyle4}" VerticalAlignment="Center" Width="100"
    							Click="Airplane_Click" Hold="Airplane_Hold" />
    						<Rectangle x:Name="AirplaneState" Fill="{StaticResource PhoneAccentBrush}"
    							HorizontalAlignment="Left" Height="10" VerticalAlignment="Bottom" Width="100"
    							Opacity="0" />
    					</Grid>
    					<Button x:Name="Accounts" Content="Button" HorizontalAlignment="Center" Height="100" Margin="5"
    						Style="{StaticResource ButtonStyle5}" VerticalAlignment="Center" Width="100"
    						Click="Accounts_Click" Hold="Accounts_Hold" />
    					<Button x:Name="LockScreen" Content="Button" HorizontalAlignment="Center" Height="100"
    						Margin="5" Style="{StaticResource ButtonStyle6}" VerticalAlignment="Center" Width="100"
    						Click="LockScreen_Click" Hold="LockScreen_Hold" />
    					<Button x:Name="Rotation" Content="Button" HorizontalAlignment="Center" Height="100" Margin="5"
    						Style="{StaticResource ButtonStyle7}" VerticalAlignment="Center" Width="100"
    						Click="Rotation_Click" Hold="Rotation_Hold" />

    				</StackPanel>
    				<StackPanel x:Name="Menu3" Orientation="Horizontal" HorizontalAlignment="Center"
    					VerticalAlignment="Center" Margin="2,0,2,2">
    					<Button x:Name="LockPhone" Content="Button" HorizontalAlignment="Center" Height="100"
    						Margin="5" Style="{StaticResource ButtonStyle8}" VerticalAlignment="Center" Width="100"
    						Click="LockPhone_Click" Hold="LockPhone_Hold" />
    					<ToggleButton x:Name="FlashLight" Content="ToggleButton" HorizontalAlignment="Center"
    						Height="100" Style="{StaticResource ToggleButtonStyle2}"
    						VerticalAlignment="Center" Width="100" Checked="FlashLight_Checked"
    						Unchecked="FlashLight_Unchecked" Cursor="Wait" Click="FlashLight_Click"
    						Margin="5" Hold="FlashLight_Hold" />
    					<Grid x:Name="BatteryInfo" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="5"
    						Hold="BatteryInfo_Hold" Tap="Battery_Click">
    						<Button x:Name="Battery" Content="Button" HorizontalAlignment="Center" Height="100"
    							Style="{StaticResource ButtonStyle18}" VerticalAlignment="Center" Width="100" />
    						<TextBlock x:Name="BatteryPercent" TextWrapping="Wrap"
    							HorizontalAlignment="Center" VerticalAlignment="Center"
    							FontFamily="Segoe WP Light" FontSize="26.667" Foreground="White" />

    					</Grid>
    					<Button x:Name="Rate" Content="Button" HorizontalAlignment="Center" Height="100" Margin="5"
    						Style="{StaticResource ButtonStyle10}" VerticalAlignment="Center" Width="100"
    						Click="Rate_Click" />

    				</StackPanel>
    				<StackPanel x:Name="Menu4" Orientation="Horizontal" HorizontalAlignment="Center"
    					VerticalAlignment="Center" Margin="2,0,2,2">
    					<Button x:Name="STOPMusic" Content="Button" HorizontalAlignment="Center" Height="100"
    						Margin="5" Style="{StaticResource ButtonStyle20}" VerticalAlignment="Center"
    						Width="100" Click="STOPMusic_Click" />
    					<Button x:Name="More" Content="Button" HorizontalAlignment="Center" Height="100" Margin="5"
    						Style="{StaticResource ButtonStyle19}" VerticalAlignment="Center" Width="100"
    						Click="More_Click" />
    					<Button x:Name="FlashLight1" Content="Button" HorizontalAlignment="Center" Height="100"
    						Margin="5" Style="{StaticResource ButtonStyle9}" VerticalAlignment="Center" Width="100"
    						Click="FlashLight_Click" Opacity="0" />
    					<Button x:Name="Battery1" Content="Button" HorizontalAlignment="Center" Height="100" Margin="5"
    						Style="{StaticResource ButtonStyle18}" VerticalAlignment="Center" Width="100"
    						Click="Battery_Click" Opacity="0" />

    				</StackPanel>

    			</StackPanel>

    		</ScrollViewer>

    	</StackPanel>

        <!--TitlePanel contains the name of the application and page title-->

        <!--ContentPanel - place additional content here-->
    </Grid>

</phone:PhoneApplicationPage>