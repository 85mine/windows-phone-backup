            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.Background>
            <ImageBrush ImageSource="/Imgs/bg.jpg"
            Stretch="Fill"/>
        </Grid.Background>
